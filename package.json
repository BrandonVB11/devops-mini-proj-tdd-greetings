    "license": "ISC",
    "bugs": {
        "url": "https://github.com/BrandonVB11/devops-mini-proj-tdd-greetings/issues"
    },
    "homepage": "https://github.com/BrandonVB11/devops-mini-proj-tdd-greetings#readme",
    "dependencies": {
        "commitizen": "^4.3.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.1",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4"
    }
